{
  "name": "Bootstrap Web App Generator",
  "version": "0.3",
  "authors": [
    "Christian Pena <dr_khriz@hotmail.com>"
  ],
  "description": "Bootstrap 4, jQuery, Modernizr, Font Awesome, Gulp, Bower",
  "private": true,
  "ignore": [
    "**/.*",
    "components",
    "node_modules",
    "vendor"
  ],
  "dependencies": {
    "modernizr": "~3",
    "jquery": "3.0.*",
    "font-awesome": "~4",
    "bootstrap": "v4-dev",
    "bootstrap-sass": "^3.3.6"
  },
  "overrides": {
    "modernizr": {
      "main": "modernizr.js"
    },
    "font-awesome": {
      "main": "fonts/*"
    }
  },
  "resolutions": {
    "jquery": "3.0.*"
  }
}
